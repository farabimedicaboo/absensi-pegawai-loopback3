{
  "name": "Attendance",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "status": {
      "type": "string",
      "required": true
    },
    "start": {
      "type": "date",
      "default": "$now"
    },
    "end": {
      "type": "date"
    },
    "isApproved": {
      "type": "boolean",
      "default": true
    },
    "employeeId": {
      "type": "string"
    },
    "createdAt": {
      "type": "date",
      "default": "$now"
    },
    "updatedAt": {
      "type": "date"
    }
  },
  "mixins": {
    "Aggregate": true
  },
  "validations": [],
  "relations": {
    "employee": {
      "type": "belongsTo",
      "model": "Employee",
      "foreignKey": "employeeId"
    }
  },
  "acls": [],
  "methods": {}
}
