{
  "name": "Employee",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "username": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string"
    },
    "createdAt": {
      "type": "date",
      "default": "$now"
    },
    "updatedAt": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "attendances": {
      "type": "hasMany",
      "model": "Attendance",
      "foreignKey": "employeeId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
